{
  "version": 2,
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" },
    { "src": "public/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/download", "dest": "/api/download.py" },
    { "src": "/api/thumbnail", "dest": "/api/thumbnail.py" }
  ]
}
